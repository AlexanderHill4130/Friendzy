<h1>Places#index</h1>
<p>Find me in app/views/places/index.html.erb</p>

<%= link_to "Home", root_path %>



<%= form_tag places_path, method: :get do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find a place to hang out"
  %>
<% end %>

<div class="container">
  <div class="row">
    <div class="col-xs-8">
      <div id="places">
        <% @places.each do |place| %>
          <h4><%= place.name %></h4>
          <p><%= place.category %></p>
          <p><%= place.address %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>
